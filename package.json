{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "partial run [ --filter '...[origin/main]...' ] --workspace-concurrency=1 build",
    "build:dev": "partial run [ --filter '...[origin/main]...' ] --workspace-concurrency=1 build:dev",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "clean": "rimraf \"**/build\" \"**/.nyc_output\" \"**/coverage\" \"**/docs\"",
    "lint": "partial run [ --filter '...[origin/main]' ] lint",
    "lint:fix": "partial run [ --filter '...[origin/main]' ] lint:fix",
    "test": "partial run [ --filter '...[origin/main]' ] test",
    "test:coverage": "partial run [ --filter '...[origin/main]' ] test:coverage",
    "test:coverage-html": "partial run [ --filter '[origin/main]' ] test:coverage-html",
    "test:coverage-lcov": "partial run [ --filter '[origin/main]' ] test:coverage-lcov",
    "test:extended": "partial run [ --filter '...[origin/main]' ] --no-bail test:extended",
    "watch": "partial run [ --filter '...[origin/main]...' ] --workspace-concurrency=Infinity watch"
  },
  "devDependencies": {
    "@changesets/cli": "2.23.1",
    "rimraf": "3.0.2",
    "scripts": "workspace:1.0.0",
    "typescript": "4.7.4"
  },
  "packageManager": "pnpm@7.5.0",
  "engines": {
    "node": ">=14"
  },
  "pnpm": {
    "overrides": {
      "@types/node": "^14.0.0",
      "istanbul-lib-processinfo": "^2.0.3"
    },
    "packageExtensions": {
      "eslint-import-resolver-webpack": {
        "peerDependenciesMeta": {
          "webpack": {
            "optional": true
          }
        }
      }
    }
  }
}
